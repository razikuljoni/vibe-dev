{
    "React Functional Component": {
        "prefix": "rfc",
        "body": [
            "import React from 'react';",
            "",
            "const ${1:ComponentName} = () => {",
            "  return (",
            "    <div>",
            "      $0",
            "    </div>",
            "  );",
            "};",
            "",
            "export default ${1:ComponentName};"
        ],
        "description": "Create a React functional component"
    },
    "React Functional Component with Props": {
        "prefix": "rfcp",
        "body": [
            "import React from 'react';",
            "",
            "interface ${1:ComponentName}Props {",
            "  $2",
            "}",
            "",
            "const ${1:ComponentName}: React.FC<${1:ComponentName}Props> = ({ $3 }) => {",
            "  return (",
            "    <div>",
            "      $0",
            "    </div>",
            "  );",
            "};",
            "",
            "export default ${1:ComponentName};"
        ],
        "description": "Create a React functional component with TypeScript props interface"
    },
    "React Hook useState": {
        "prefix": "ust",
        "body": [
            "const [${1:state}, set${1/(.*)/${1:/capitalize}/}] = useState($2);"
        ],
        "description": "React useState hook"
    },
    "React Hook useEffect": {
        "prefix": "uef",
        "body": ["useEffect(() => {", "  $0", "}, [$1]);"],
        "description": "React useEffect hook"
    },
    "React Hook useEffect with cleanup": {
        "prefix": "uefc",
        "body": [
            "useEffect(() => {",
            "  $1",
            "",
            "  return () => {",
            "    $2",
            "  };",
            "}, [$0]);"
        ],
        "description": "React useEffect hook with cleanup function"
    },
    "React Hook useContext": {
        "prefix": "uctx",
        "body": ["const ${1:context} = useContext(${2:ContextName});"],
        "description": "React useContext hook"
    },
    "React Hook useReducer": {
        "prefix": "ured",
        "body": [
            "const [${1:state}, ${2:dispatch}] = useReducer(${3:reducer}, ${4:initialState});"
        ],
        "description": "React useReducer hook"
    },
    "React Hook useMemo": {
        "prefix": "umem",
        "body": [
            "const ${1:memoizedValue} = useMemo(() => {",
            "  return $2;",
            "}, [$3]);"
        ],
        "description": "React useMemo hook"
    },
    "React Hook useCallback": {
        "prefix": "ucb",
        "body": [
            "const ${1:memoizedCallback} = useCallback(() => {",
            "  $2",
            "}, [$3]);"
        ],
        "description": "React useCallback hook"
    },
    "React Hook useRef": {
        "prefix": "uref",
        "body": ["const ${1:ref} = useRef($2);"],
        "description": "React useRef hook"
    },
    "React Custom Hook": {
        "prefix": "rhook",
        "body": [
            "import { useState, useEffect } from 'react';",
            "",
            "const use${1:HookName} = ($2) => {",
            "  const [${3:state}, set${3/(.*)/${1:/capitalize}/}] = useState($4);",
            "",
            "  useEffect(() => {",
            "    $5",
            "  }, [$6]);",
            "",
            "  return { ${3:state}, set${3/(.*)/${1:/capitalize}/} };",
            "};",
            "",
            "export default use${1:HookName};"
        ],
        "description": "Create a custom React hook"
    },
    "React Context Provider": {
        "prefix": "rctx",
        "body": [
            "import React, { createContext, useContext, useState } from 'react';",
            "",
            "interface ${1:Context}ContextType {",
            "  $2",
            "}",
            "",
            "const ${1:Context}Context = createContext<${1:Context}ContextType | undefined>(undefined);",
            "",
            "export const ${1:Context}Provider: React.FC<{ children: React.ReactNode }> = ({ children }) => {",
            "  const [${3:state}, set${3/(.*)/${1:/capitalize}/}] = useState($4);",
            "",
            "  const value = {",
            "    ${3:state},",
            "    set${3/(.*)/${1:/capitalize}/},",
            "    $5",
            "  };",
            "",
            "  return (",
            "    <${1:Context}Context.Provider value={value}>",
            "      {children}",
            "    </${1:Context}Context.Provider>",
            "  );",
            "};",
            "",
            "export const use${1:Context} = () => {",
            "  const context = useContext(${1:Context}Context);",
            "  if (!context) {",
            "    throw new Error('use${1:Context} must be used within a ${1:Context}Provider');",
            "  }",
            "  return context;",
            "};"
        ],
        "description": "Create a React Context with Provider and hook"
    },
    "React Component with forwardRef": {
        "prefix": "rfref",
        "body": [
            "import React, { forwardRef } from 'react';",
            "",
            "interface ${1:ComponentName}Props {",
            "  $2",
            "}",
            "",
            "const ${1:ComponentName} = forwardRef<HTML${3:Div}Element, ${1:ComponentName}Props>(({ $4 }, ref) => {",
            "  return (",
            "    <${5:div} ref={ref}>",
            "      $0",
            "    </${5:div}>",
            "  );",
            "});",
            "",
            "${1:ComponentName}.displayName = '${1:ComponentName}';",
            "",
            "export default ${1:ComponentName};"
        ],
        "description": "Create a React component with forwardRef"
    }
}
